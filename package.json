{
  "name": "puppeteer",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "memoryTest": "node ./dist/test/memoryTest.js",
    "build": "rm -rf ./dist/* && tsc",
    "test": "yarn build && node ./dist/test/index.js --mode=develop --useChrome=no --useScreenshot=yes",
    "testDev": "yarn build && node ./dist/test/index.js --mode=develop --useChrome=yes", 
    "product": "yarn build && node ./dist/index.js --useScreenshot=yes",
    "deploy": "yarn build && rsync -auvhp --delete --exclude=node_modules --exclude=dist ./* aws-redot-test:~/puppeteer",
    "lint": "eslint src --ext .ts && prettier --write -l \"./src/**/*\""
  },
  "dependencies": {
    "@types/cheerio": "^0.22.17",
    "@types/puppeteer": "^2.0.1",
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "dateformat": "^3.0.3",
    "node-xlsx": "^0.15.0",
    "prettier": "^2.0.4",
    "puppeteer": "^2.1.1",
    "tedis": "^0.1.12"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-alloy": "^3.7.1",
    "typescript": "^3.8.3"
  }
}
